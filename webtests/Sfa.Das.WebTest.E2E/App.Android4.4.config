<?xml version="1.0" encoding="utf-8" ?>
<!-- For more information on using transformations 
     see the web.config examples at http://go.microsoft.com/fwlink/?LinkId=214134. -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="RemoteUrl" value="http://hub.browserstack.com:80/wd/hub/" xdt:Locator="Match(key)" xdt:Transform="Insert" />
    <add key="service.url" value="http://das-pp-search.cloudapp.net" xdt:Locator="Match(key)" xdt:Transform="Replace" />
    <add key="browser" value="Android" xdt:Locator="Match(key)" xdt:Transform="Replace" />
    <add key="device" value="Samsung Galaxy S5" xdt:Locator="Match(key)" xdt:Transform="Insert" />
    <add key="os" value="android" xdt:Locator="Match(key)" xdt:Transform="Insert" />
    <add key="browser_version" value="4.4" xdt:Locator="Match(key)" xdt:Transform="Insert" />

    <add key="data.framework.JOBROLE" value="Construction Building: Wood Occupations" xdt:Locator="Match(key)" xdt:Transform="Replace"/>
    <add key="data.framework.Postcode" value="EX4 4JS" xdt:Locator="Match(key)" xdt:Transform="Replace"/>
  </appSettings>
</configuration>