@using Sfa.Das.Sas.Shared.Components.ViewModels.Css.Interfaces
@model Sfa.Das.Sas.Shared.Components.ViewComponents.Basket.SaveBasketViewModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@inject ICssViewModel CssViewModel

<form asp-controller="Basket" asp-action="DeleteBasketViewCache" method="post" target="_blank">
    <input type="hidden" id="BasketId" name="BasketId" value="@Model.BasketId" />
    <input type="hidden" id="SaveBasketUrl" name="SaveBasketUrl" value="@Model.SaveBasketUrl" />
    <button type="submit" class="@(CssViewModel.Button)">CREATE AN ACCOUNT</button>
</form>