<?xml version="1.0" encoding="utf-8"?>
<ServiceConfiguration serviceName="Sfa.Das.CloudService.Web" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceConfiguration" xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <Role name="Sfa.Eds.Das.Web">
    <Instances count="#{CloudService.InstanceCount}" xdt:Transform="Replace" />
  </Role>
  <NetworkConfiguration>
    <AddressAssignments xdt:Transform="Replace">
      <InstanceAddress roleName="Sfa.Eds.Das.Web">
        <Subnets>
          <Subnet name="#{azure.vnet.subnet.name}" />
        </Subnets>
      </InstanceAddress>
      <ReservedIPs>
        <ReservedIP name="#{AzureCloudService.ReservedIpName}"/>
      </ReservedIPs>
    </AddressAssignments>
  </NetworkConfiguration>
</ServiceConfiguration>
