<?xml version="1.0" encoding="utf-8"?>
<ServiceConfiguration serviceName="Sfa.Das.Sas.Web.CloudService" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceConfiguration" osFamily="4" osVersion="*" schemaVersion="2015-04.2.6">
  <Role name="Sfa.Das.Sas.Web">
    <Instances count="1" />
    <ConfigurationSettings>
      <Setting name="Microsoft.WindowsAzure.Plugins.Diagnostics.ConnectionString" value="#{StorageConnectionString}" />
      <Setting name="PostcodeUrl" value="http://dashttpstatuscodefake.azurewebsites.net/api/status/500/" />
      <Setting name="SurveyUrl" value="https://www.surveymonkey.co.uk/r/F3LCBG6" />
      <Setting name="ElasticServerUrls" value="http://127.0.0.1:9200,http://192.168.99.100:9200,http://docker.local:9200" />
      <Setting name="SatisfactionSourceUrl" value="https://www.gov.uk/government/statistical-data-sets/fe-choices-performance-indicators" />
      <Setting name="CookieGoogleUrl" value="https://tools.google.com/dlpage/gaoptout" />
      <Setting name="CookieImprovementUrl" value="https://insidegovuk.blog.gov.uk/2015/03/26/new-tool-to-see-trending-searches/" />
      <Setting name="CookieApplicationInsightsUrl" value="https://azure.microsoft.com/en-us/documentation/articles/app-insights-data-retention-privacy/" />
      <Setting name="CookieAboutUrl" value="http://www.aboutcookies.org/" />
      <Setting name="AchievementRateUrl" value="https://www.gov.uk/government/statistical-data-sets/sfa-national-achievement-rates-tables-2014-to-2015" />
    </ConfigurationSettings>
    <Certificates>
      <Certificate name="cert" thumbprint="18E443B4CFC7A4652B497F241D31866DC7E3D4F7" thumbprintAlgorithm="sha1" />
    </Certificates>
  </Role>
  <NetworkConfiguration>
    <!-- Name of the target Virtual Network -->
    <VirtualNetworkSite name="#{azure.vnet.name}" />
    <!-- Associating a Role to a Specific Subnet by name -->
    <AddressAssignments>
      <!-- See Transform.ServiceConfiguration.Cloud.config -->
    </AddressAssignments>
    <AccessControls>
      <AccessControl name="RoleAccess">
        <Rule action="permit" description="AllowedAccess" order="100" remoteSubnet="0.0.0.0/0" />
        <Rule action="deny" description="DenyAccess" order="200" remoteSubnet="0.0.0.0/0" />
      </AccessControl>
    </AccessControls>
    <EndpointAcls>
      <EndpointAcl role="Sfa.Das.Sas.Web" endPoint="Endpoint1" accessControl="RoleAccess" />
    </EndpointAcls>
  </NetworkConfiguration>
</ServiceConfiguration>